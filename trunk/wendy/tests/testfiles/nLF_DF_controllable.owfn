
PLACE
INTERNAL
  p1,
  p2,
  p3,
  p4;

INPUT
  a,
  b;

OUTPUT
  c;

INITIALMARKING
  p1:	1
 ;

FINALCONDITION
  p2=1 AND ALL_OTHER_PLACES_EMPTY
 ;

TRANSITION t1	 { ?a }
CONSUME
  p1:	1,
  a: 1;
PRODUCE
  p2 : 1;

TRANSITION t2	 { tau }
CONSUME
  p1:	1;
PRODUCE
  p3 : 1;

TRANSITION t3	 { ?b }
CONSUME
  p3:	1,
  b: 1;
PRODUCE
  p4 : 1;

TRANSITION t4	 { !c }
CONSUME
  p4:	1;
PRODUCE
  p3 : 1,
  c: 1;