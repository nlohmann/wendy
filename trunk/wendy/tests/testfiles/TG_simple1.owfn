{ simple sequence - one send event}

PLACE
INTERNAL
  p0,  { initial }
  p1,		
  p2,
  p3,
  p4;
INPUT
  a,
  c,
  d;
OUTPUT
  x;

INITIALMARKING
  p0:	1
 ;

FINALMARKING
  p4:	1
 ;

TRANSITION t1	 { ?a }
CONSUME
  p0:	1,
  a:    1;	 
PRODUCE
  p1:	1;


TRANSITION t2	 { !x }
CONSUME
  p1:	1;
PRODUCE
  p2 :  1,
  x  :  1;

TRANSITION t3	 { ?c }
CONSUME
  p2:	1,
  c:    1;
PRODUCE
  p3 :  1;


TRANSITION t4	 { ?d }
CONSUME
  p3:	1,
  d:    1;
PRODUCE
  p4 :  1;


{ END OF FILE }
