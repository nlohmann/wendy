PLACE
INTERNAL p8, p9, p10, p11, p12, p13, p14;
INPUT regPass, regID, noFingerprint, fingerprint;
OUTPUT price, printDoc;

INITIALMARKING p8:1;
FINALCONDITION (p14=1 AND ALL_OTHER_PLACES_EMPTY);

TRANSITION i
CONSUME p8, regPass;
PRODUCE p9;

TRANSITION j
CONSUME p8, regID;
PRODUCE p10;

TRANSITION k
CONSUME p9;
PRODUCE p11, price;

TRANSITION l
CONSUME p10;
PRODUCE p12, price;

TRANSITION m
CONSUME p11, noFingerprint;
PRODUCE p13;

TRANSITION n
CONSUME p11, fingerprint;
PRODUCE p13;

TRANSITION o
CONSUME p12, fingerprint;
PRODUCE p13;

TRANSITION p
CONSUME p13;
PRODUCE p14, printDoc;
